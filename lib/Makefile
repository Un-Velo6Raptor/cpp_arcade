all:
		make -C SFML
		make -C NCurses
		make -C SDL2

clean:
		make -C SFML clean
		make -C NCurses clean
		make -C SDL2 clean
        
fclean:
		make -C SFML fclean
		make -C NCurses fclean
		make -C SDL2 fclean

re:
		make -C SFML re
		make -C NCurses re
		make -C SDL2 re

.PHONY: all clean fclean re
